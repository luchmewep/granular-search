{
    "name": "luchmewep/granular-search",
    "description": "Granular Search and Aggregation provides four (4) usable traits for model controllers which are useful for searching and aggregation.",
    "license": "license",
    "authors": [
        {
            "name": "James Carlo Luchavez",
            "email": "mrstudyfirst@gmail.com",
            "homepage": "https://www.facebook.com/luchmewep/"
        }
    ],
    "homepage": "https://github.com/luchmewep/granular-search",
    "keywords": [
        "Laravel",
        "GranularSearch"
    ],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Luchmewep\\GranularSearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Luchmewep\\GranularSearch\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Luchmewep\\GranularSearch\\GranularSearchServiceProvider"
            ],
            "aliases": {
                "GranularSearch": "Luchmewep\\GranularSearch\\Facades\\GranularSearch"
            }
        }
    }
}
